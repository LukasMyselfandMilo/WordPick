      //New to page script part
      var LastPick = getCookie("LastPick")
      if (LastPick == null)
      {
        idCookie('LastPick', 'List')

        //Picks.toString();
      }
      else
      {
        var json_str = getCookie('LastPick')
        var Picks = JSON.parse(json_str)
        document.getElementById("List").value = Picks.toString();
      }
