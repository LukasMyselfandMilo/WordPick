      //New to page script part
      var Picks = ["Chocolate", "Vanilla", "Bannana", "Carimel", "Mint", "Rocky Road", "Cookies and Cream"]
      var json_str = JSON.stringify(Picks)
      var LastPick = getCookie("LastPick")
      if (LastPick == null)
      {
        createCookie('LastPick', json_str, 30)
        thingy = document.getElementById("List")
        thingy.value = Picks.toString();
      }
      else
      {
        var json_str = getCookie('LastPick')
        var Picks = JSON.parse(json_str)
        document.getElementById("List").value = Picks.toString();
      }
