function idCookie(cookieName) {
inputId = 'List'
var input = document.getElementById(inputId).value
input = input.replace("\"", "");
OutputArray = input.split(",")
var cookieOutput = JSON.stringify(input)
cookieOutput = cookieOutput.replace("\"", "");
cookieOutput = cookieOutput.replace("\"", "");
cookieOutput = cookieOutput.replace("\"", "");
cookieOutput = cookieOutput.replace("\"", "");
createCookie(cookieName, cookieOutput, -1)
createCookie(cookieName, cookieOutput, 30)
}
